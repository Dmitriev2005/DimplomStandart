<Window x:Class="DimplomStandart.Windows.Report.FRDOWindows.FRDO"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DimplomStandart.Windows.Report.FRDOWindows"
        mc:Ignorable="d"
        Title="Выгрузка ФРДО" Height="230" Width="300">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <StackPanel Width="200">
                <StackPanel>
                    <Label Content="С"/>
                </StackPanel>
                <StackPanel>
                    <DatePicker x:Name="dtpStartDate"/>
                </StackPanel>
                <StackPanel>
                    <Label Content="По"/>
                </StackPanel>
                <StackPanel>
                    <DatePicker x:Name="dtpEndDate"/>
                </StackPanel>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0,20,0,0">
                    <Button Content="Ок" Width="70" x:Name="btnOk" Click="btnOk_Click"/>
                    <Button Content="Отмена" Margin="40,0,0,0" Width="70" x:Name="btnCancel" Click="btnCancel_Click"/>
                </StackPanel>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
